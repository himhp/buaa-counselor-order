/* list.wxss */
.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 2.2rem;
  align-items: center;
}
.td {
  width: 40%;
  justify-content: center;
  text-align: center;
  font-size: 28rpx;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 30%;
  justify-content: center;
  background: #3399FF;
  color: #fff;
  display: flex;
  height: 2.2rem;
  align-items: center;
  font-size: 32rpx;
}